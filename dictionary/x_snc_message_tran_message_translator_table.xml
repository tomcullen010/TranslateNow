<?xml version="1.0" encoding="UTF-8"?>
<database>
    <element label="Message Translator Log" max_length="40" name="x_snc_message_tran_message_translator_table" type="collection">
        <element choice="1" label="Applications" max_length="40" name="applications" reference="sys_ui_message" reference_key="Application" type="reference">
            <choice/>
        </element>
        <element label="As of Date" max_length="40" name="as_of_date" type="glide_date"/>
        <element choice="2" label="Translate To Language" mandatory="true" max_length="40" name="current_languages" reference="sys_language" reference_qual="active=true^EQ" reference_qual_condition="active=true^EQ" type="reference">
            <choice/>
        </element>
        <element hint="Number of messages created outside of the plugin messages" label="Custom Message Count" max_length="40" name="custom_messages" read_only="true" type="integer"/>
        <element label="Google Translate API Key" max_length="40" name="google_translate_api_key" read_only="true" type="string"/>
        <element label="Google Translate (From) Language" max_length="40" name="google_translate_from_language" read_only="true" type="string"/>
        <element hint="To Activate a language plugin, select the language via Lookup.  Once selected, you can use the record edit to activate the plugin." label="Activate Languages" max_length="32" name="inactive_languages" reference="v_plugin" reference_qual="nameSTARTSWITHi18N^nameLIKEtranslations^active=inactive^EQ" reference_qual_condition="nameSTARTSWITHi18N^nameLIKEtranslations^active=inactive^EQ" type="reference"/>
        <element label="Message Log" max_length="500" name="message_log" read_only="true" type="string"/>
        <element default="javascript:global.getNextObjNumberPadded();" label="Number" max_length="40" name="number" type="string"/>
        <element label="Process Time" max_length="40" name="process_time" type="glide_duration"/>
        <element label="Search Description" max_length="40" name="search_description" type="string"/>
        <element label="Search Language" max_length="40" name="search_language" type="string"/>
        <element label="Search Property" max_length="40" name="search_property" type="string"/>
        <element label="System Default Language" max_length="40" name="system_default_language" read_only="true" type="string"/>
        <element label="Translated Message Count" max_length="40" name="translated_message_count" type="integer"/>
        <element label="Translation End" max_length="40" name="translation_end" type="glide_date_time"/>
        <element label="Translation Start" max_length="40" name="translation_start" type="glide_date_time"/>
        <element default="true" label="Update Custom Messages Only" max_length="40" name="update_custom_messages_only" type="boolean"/>
        <index name="index">
            <element name="inactive_languages"/>
        </index>
    </element>
</database>
